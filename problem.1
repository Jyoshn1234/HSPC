#include<stdio.h>
#include<math.h>
typedef struct point
{
float x;
float y;
}Point;
typedef struct rectangle
{
Point p1,p2,p3;
float area;
}Rectangle;

int input_n();
Point input_point();
Rectangle input_rectangle();
void input_n_rectangles(int n, Rectangle r[n]);
void find_areas(int n, Rectangle r[n]);
void output(int n,Rectangle r[n]);

int main()
{
int n;
n = input_n();
Rectangle r[n];
input_n_rectangles(n,r);
find_areas(n,r);
output(n,r);
}

int input_n()
{
int n;
printf("Enter the no.of rectangles");
scanf("%d",&n);
return n;
}

Point input_point()
{
Point p;
printf("Enter x");
scanf("%f",&p.x);
printf("Enter y");
scanf("%f",&p.y);
return p;
}
Rectangle input_rectangle()
{
Rectangle r;
r.p1 = input_point();
r.p2 = input_point();
r.p3 = input_point();
return r;
}
void input_n_rectangles(int n, Rectangle r[n])
{
for(int i=0;i<n;i++)
{
r[i] = input_rectangle();
}
}
void find_areas(int n, Rectangle r[n])
{
  for (int i=0;i<n;i++)
    {
      r[i].area=fabs( (r[i].p1.x*(r[i].p2.y - r[i].p3.y)) + (r[i].p2.x*(r[i].p3.y - r[i].p1.y)) + (r[i].p3.x*(r[i].p1.y - r[i].p2.y)) );
    }
}
void output(int n,Rectangle r[n])
{
for(int i=0;i<n;i++)
{
printf("The area of the rectangles with points (%f,%f), (%f,%f)and (%f,%f) is %f,r[i].p1.x,r[i]p1.y,r[i].p2.x,r[i].p2.y,r[i].p3.x,r[i].p3.y,r[i].area);
}
}
